{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
 
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.debug.json",
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [ 
    "@typescript-eslint",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error"
    ]
  }
}
